{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-native/all"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-global-assign": "warn",
    "no-console": "off",
    "react-native/no-color-literals": "warn"
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "globals": {
    "fetch": false
  }
}
